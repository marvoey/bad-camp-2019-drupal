{%
set classes = [
dom_id ? 'js-view-dom-id-' ~ dom_id,
]
%}
<div{{ attributes.addClass(classes) }}>
    {{ title_prefix }}
    {{ title }}
    {#{{ title_suffix }}#}
    <div class="row">
        <div class="col-sm-6 col-md-12">
            <div class="side-custom-menu">
                <h2>
                    <i class="fa fa-bars"></i>
                    {% if header %}
                        {{ header }}
                    {% endif %}
                </h2>

                {{ rows }}
            </div>
        </div>
        <div class="col-sm-6 col-md-12">
            {% if footer %}
                {{ footer }}
            {% endif %}
        </div>
    </div>

    {{ exposed }}
    {{ attachment_before }}

    {{ empty }}
    {{ pager }}

    {{ attachment_after }}
    {{ more }}



    {{ feed_icons }}
</div>
